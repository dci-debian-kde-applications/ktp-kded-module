msgid "Contact request denied"
msgstr "Zahtjev za kontakt odbijen"

#: contact-request-handler.cpp:374
#, kde-format
msgid "%1 will not be able to see when you are online"
msgstr "%1 neće moći da vidi kada ste na vezi"

